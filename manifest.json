{
  "manifest_version": 3,
  "name": "Sundial",
  "version": "1.0.0",
  "description": "See the current time in your favorite locations, right on your new tab.",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://geocoding-api.open-meteo.com/*"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
